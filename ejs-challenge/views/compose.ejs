<%- include("partials/header") -%>

<h1>New Entry</h1>

<form action="/compose" method="post">
    <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle">

    <label>Post</label>
    <textarea class="form-control" name="postBody" cols="30" rows="5"></textarea>
    </div>

    <button class="btn entry" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer") -%>